<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colors-->
    <Color x:Key="DeepGreen">#196e3b</Color>
    <Color x:Key="Green">#99fa93</Color>
    <Color x:Key="Orange">#f3a300</Color>
    <Color x:Key="LightGrey">#c6c6c6</Color>
    <Color x:Key="Grey">#666666</Color>

    <!--Brushes-->
    <SolidColorBrush x:Key="BrushDeepGreen" Color="{StaticResource DeepGreen}"/>
    <SolidColorBrush x:Key="BrushGreen" Color="{StaticResource Green}"/>
    <SolidColorBrush x:Key="BrushLightGrey" Color="{StaticResource LightGrey}"/>
    <SolidColorBrush x:Key="BrushOrange" Color="{StaticResource Orange}"/>

</ResourceDictionary>